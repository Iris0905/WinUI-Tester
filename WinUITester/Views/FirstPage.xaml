<Page    x:Class="WinUITester.Views.FirstPage"    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"     xmlns:charts="using:DevExpress.WinUI.Charts"     xmlns:controls="using:DevExpress.WinUI.Controls"    mc:Ignorable="d"    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">    <Grid Padding="48">        <Grid.RowDefinitions>            <RowDefinition Height="Auto" />            <RowDefinition Height="8*" />            <RowDefinition Height="*" />        </Grid.RowDefinitions>        <TextBlock Margin="48,0,0,0"                       Text="200 000 Points (100 000 Points per Series)" />        <charts:CartesianChart x:Name="chart"                                   Grid.Row="1"                                   Margin="30,50,30,25"                                   AllowAxisXZoomScroll="True"                                   ToolTipEnabled="False">            <charts:CartesianChart.AxisX>                <charts:AxisX ShowMajorGridlines="True" />            </charts:CartesianChart.AxisX>            <charts:CartesianChart.AxisY>                <charts:AxisY ShowLabels="False" ShowMajorGridlines="False" />            </charts:CartesianChart.AxisY>            <charts:CartesianChart.Series>                <charts:Series Data="{x:Bind ViewModel.Data1}" DisplayName="Series 1">                    <charts:Series.View>                        <charts:LineSeriesView />                    </charts:Series.View>                </charts:Series>                <charts:Series Data="{x:Bind ViewModel.Data2}" DisplayName="Series 2">                    <charts:Series.View>                        <charts:LineSeriesView />                    </charts:Series.View>                </charts:Series>            </charts:CartesianChart.Series>        </charts:CartesianChart>        <controls:RangeControl Grid.Row="2"                                   MinHeight="40"                                   Margin="30,-10,30,15"                                   Background="Transparent"                                   ShowRangeThumbs="True">            <charts:ChartRangeControlClient Chart="{Binding ElementName=chart}" />        </controls:RangeControl>    </Grid></Page>
